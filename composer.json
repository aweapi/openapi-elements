{
  "name": "aweapi/openapi-elements",
  "description": "OpenApi PHP Objects",
  "license": "MIT",
  "homepage": "https://github.com/aweapi/openapi-elements",
  "keywords": ["openapi", "php"],
  "authors": [
    {
      "name": "Veaceslav Medvedev",
      "email": "slavcopost@gmail.com",
      "homepage": "https://github.com/slavcodev"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.2",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15",
    "phpunit/phpunit": "^8.4",
    "psalm/plugin-phpunit": "^0.7",
    "scrutinizer/ocular": "^1.6",
    "vimeo/psalm": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Aweapi\\Openapi\\": ["src"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aweapi\\Tests\\Openapi\\": ["tests"]
    }
  },
  "scripts": {},
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
